#!/usr/bin/python2.7
# -*- coding: utf-8 -*-

import sys
from string import ascii_letters


r = bytes()
for letter in ' '.join(sys.argv[1:]):
  
  val = ord(letter)
  if val > 0x21 and val < 0x7f:
    r += unichr(0xfee0+val)
  elif val == 0x20:
    r += unichr(0x3000)
  else:
    r += chr(val)

print r
